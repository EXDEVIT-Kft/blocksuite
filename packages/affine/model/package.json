    "directory": "packages/affine/model"
  },
  "scripts": {
    "build": "tsc",
    "test:unit": "nx vite:test --run --passWithNoTests",
    "test:unit:coverage": "nx vite:test --run --coverage",
    "test:e2e": "playwright test"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@algogrind/block-std": "workspace:*",
