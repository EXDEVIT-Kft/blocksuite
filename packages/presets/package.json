{
  "name": "@algogrind/presets",
  "version": "0.18.6",
  "description": "Prebuilt BlockSuite editors and opt-in additional UI components.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/toeverything/blocksuite.git",
    "directory": "packages/presets"
  },
  "scripts": {
    "build": "tsc",
    "test:unit": "nx vite:test --browser.headless --run",
    "test:debug": "PWDEBUG=1 npx vitest"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@algogrind/affine-block-surface": "workspace:*",
    "@algogrind/affine-model": "workspace:*",
    "@algogrind/affine-shared": "workspace:*",
    "@algogrind/block-std": "workspace:*",
    "@algogrind/blocks": "workspace:*",
    "@algogrind/global": "workspace:*",
    "@algogrind/inline": "workspace:*",
    "@algogrind/store": "workspace:*",
    "@floating-ui/dom": "^1.6.10",
    "@lottiefiles/dotlottie-wc": "^0.2.16",
    "@preact/signals-core": "^1.8.0",
    "@toeverything/theme": "^1.1.1",
    "lit": "^3.2.0",
    "zod": "^3.23.8"
  },
  "exports": {
    ".": "./src/index.ts",
    "./effects": "./src/effects.ts"
  },
  "publishConfig": {
    "access": "restricted",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "module": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./effects": {
        "module": "./dist/effects.js",
        "types": "./dist/effects.d.ts",
        "import": "./dist/effects.js"
      }
    }
  },
  "files": [
    "src",
    "dist",
    "themes",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
